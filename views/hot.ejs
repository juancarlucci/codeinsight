<!DOCTYPE html>
<html lang="en">
  <head>
    <meta charset="utf-8">
    <meta name="viewport" content="width=device-width, initial-scale=1">

    <title>codeInsight</title>

    <!-- STYLESHEETS -->
    <link rel="stylesheet" href="/styles/styles.css">

    <!-- VENDOR SCRIPTS -->
    <script src="/scripts/d3.min.js"></script>
    <script src="/scripts/jquery3.3.1.min.js"></script>
    <script type='text/javascript' src="http://code.jquery.com/ui/1.11.0/jquery-ui.min.js"></script>
    <script type='text/javascript' src="http://code.jquery.com/ui/1.11.0/themes/smoothness/jquery-ui.css"></script>

    <!-- APPLICATION SCRIPTS -->
    <!-- <script src="/scripts/app.js"></script> -->
  </head>
  <body>
    <!-- <div id="nav">Nav</div> -->
    <%- include('nav'); %>
    <container class "flex-contaier">
      <div class="selectors">
        <button id="language">Language split</button>
        <button id="all">All</button>
        <!-- <button id="concentric">Linear</button> -->
      </div>
      <div class="ui-widget selectors">
        <input id="search" placeholder="search repos...">
        <button id="searchbutton" type="button">Search</button>
      </div>
    </container>
    <container class="main flex-contaier">
      <section class="popular-list-container">
        <h2 class="popular-title">hottest repos</h2>
        <small>scroll for more</small>
        <div id="popular-list"></div>

      </section>

      <article id="stars-layout">
        <h1>what's hot on github</h1>
      </article>

    </container>
    <!-- <script src="/scripts/d3.js" defer></script> -->
    <script src="/scripts/hot.js" defer></script>
  </body>
</html>
